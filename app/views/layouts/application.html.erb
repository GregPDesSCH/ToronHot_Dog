<%#
  ToronHot Dog
  View Templates - Application Template

  This is where the HTML parsing starts, where any panes that are supported by
  this website are placed where the <yield> tag is.

  VERSION 1.0
  Start Date: February 27, 2018
  End Date: April , 2018

  File Name: application.html.erb

  Original Source Code © 2018 Gregory Desrosiers. All rights reserved.

%>

<!DOCTYPE html>
<html>
  <head>
    <title>ToronHot Dog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag('bootstrap.min.css') %>
    <%= stylesheet_link_tag('bootstrap-grid.min.css') %>
    <%= stylesheet_link_tag('bootstrap-reboot.min.css') %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag('jquery-3.3.1.min.js') %>
    <%= javascript_include_tag('toronhot_dog.js') %>
    <%= javascript_include_tag('bootstrap.min.js') %>
    <%= javascript_include_tag('bootstrap.bundle.min.js') %>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GOOGLE_MAPS_DEVELOPMENT_API_KEY'] %>&callback=initMap">
    </script>
  </head>

  <body>
    <div class="container-fluid">
      <h1><span id="torontoHotDogLeftSide">Toron</span><span id="torontoHotDogCenterSide">Hot</span> <span id="torontoHotDogRightSide">Dog</span></h1>
      <div id="mainDiv">
        <%= yield %>
      </div>

      <div>
        <div>Maps powered by Google. Google Maps and Google are trademarks of Google. © 2018 Google.</div>
        <div>&copy; 2018 Gregory Desrosiers / University of Waterloo. All rights reserved.</div>
      </div>
    </div>
  </body>
</html>
